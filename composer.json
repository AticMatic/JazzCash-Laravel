{
    "name": "aticmatic/laravel-jazzcash",
    "description": "Laravel package for seamless integration with the JazzCash Mobile Wallet REST API (v2.0).",
    "keywords": [
        "laravel",
        "jazzcash",
        "payment",
        "gateway",
        "mobile wallet",
        "pakistan",
        "rest",
        "api"
    ],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Aticmatic",
            "email": "aticmatic.official@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Aticmatic\\JazzCash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aticmatic\\JazzCash\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aticmatic\\JazzCash\\Providers\\JazzCashServiceProvider"
            ],
            "aliases": {
                "JazzCash": "Aticmatic\\JazzCash\\Facades\\JazzCash"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    }
}